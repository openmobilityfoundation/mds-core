{
  "name": "@mds-core/mds-compliance-service",
  "version": "0.3.2",
  "description": "Compliance Service",
  "keywords": [
    "mds",
    "compliance"
  ],
  "license": "UNLICENSED",
  "author": "Lacuna Technologies",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "pnpm watch server",
    "pretest": "pnpm typeorm -- schema:drop",
    "test": "DOTENV_CONFIG_PATH=../../.env jest --runInBand",
    "ts-node": "DOTENV_CONFIG_PATH=../../.env node --experimental-repl-await -r ts-node/register -r dotenv/config",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "watch": "pnpm build && nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-db": "workspace:0.3.2",
    "@mds-core/mds-logger": "workspace:0.2.4",
    "@mds-core/mds-providers": "workspace:0.1.34",
    "@mds-core/mds-repository": "workspace:0.1.10",
    "@mds-core/mds-rpc-common": "workspace:0.1.10",
    "@mds-core/mds-schema-validators": "workspace:0.3.1",
    "@mds-core/mds-service-helpers": "workspace:0.3.3",
    "@mds-core/mds-types": "workspace:0.4.0",
    "@mds-core/mds-utils": "workspace:0.1.36",
    "joi": "17.4.0",
    "typeorm": "0.2.34"
  }
}
