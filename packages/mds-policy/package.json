{
  "name": "@mds-core/mds-policy",
  "version": "0.2.0",
  "description": "MDS Policy API",
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "generate": "pnpm generate:schema",
    "generate:schema": "ts-node ./generate-schemas.ts && git add schema-gen",
    "start": "PATH_PREFIX=/policy pnpm watch server",
    "test": "PATH_PREFIX=/policy DOTENV_CONFIG_PATH=../../.env jest",
    "ts-node": "pnpm build && DOTENV_CONFIG_PATH=../../.env ts-node -r dotenv/config",
    "watch": "nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-api-helpers": "0.1.36",
    "@mds-core/mds-api-server": "0.2.1",
    "@mds-core/mds-db": "0.2.4",
    "@mds-core/mds-logger": "0.2.3",
    "@mds-core/mds-policy-service": "0.3.0",
    "@mds-core/mds-providers": "0.1.33",
    "@mds-core/mds-schema-validators": "0.2.3",
    "@mds-core/mds-types": "0.3.2",
    "@mds-core/mds-utils": "0.1.34",
    "@types/express": "4.17.12",
    "express": "4.17.1",
    "http-status-codes": "2.1.4"
  },
  "devDependencies": {
    "@mds-core/mds-geography-service": "0.1.8",
    "@mds-core/mds-test-data": "0.1.34"
  }
}
