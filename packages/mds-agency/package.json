{
  "name": "@mds-core/mds-agency",
  "version": "0.1.4",
  "description": "MDS Agency API",
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "PATH_PREFIX=/agency pnpm watch server",
    "test": "PATH_PREFIX=/agency DOTENV_CONFIG_PATH=../../.env nyc --use-spawn-wrap ts-mocha --project ../../tsconfig.json --exit",
    "ts-node": "DOTENV_CONFIG_PATH=../../.env ts-node -r dotenv/config",
    "watch": "nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-agency-cache": "0.2.3",
    "@mds-core/mds-api-helpers": "0.1.36",
    "@mds-core/mds-api-server": "0.2.1",
    "@mds-core/mds-db": "0.2.4",
    "@mds-core/mds-ingest-service": "0.3.3",
    "@mds-core/mds-logger": "0.2.3",
    "@mds-core/mds-providers": "0.1.33",
    "@mds-core/mds-schema-validators": "0.2.3",
    "@mds-core/mds-service-helpers": "0.3.1",
    "@mds-core/mds-stream": "0.1.34",
    "@mds-core/mds-types": "0.3.2",
    "@mds-core/mds-utils": "0.1.34",
    "@types/express": "4.17.12",
    "ajv": "8.6.1",
    "express": "4.17.1"
  },
  "devDependencies": {
    "@mds-core/mds-test-data": "0.1.34"
  }
}
