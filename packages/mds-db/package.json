{
  "name": "@mds-core/mds-db",
  "version": "0.1.5",
  "description": "Mobility Data Specification database interface",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "keywords": [
    "mds",
    "database"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "test": "yarn test:eslint && yarn test:unit",
    "test:eslint": "eslint --fix --ignore-path ../../.gitignore '**/*.ts'",
    "test:unit": "nyc --check-coverage --exclude tests --extension .ts --lines 0 ts-mocha --project ../../tsconfig.json --require tsconfig-paths/register --timeout 6000 --require source-map-support/register --recursive tests/**/*.ts && nyc report --reporter=html"
  },
  "author": "City of Los Angeles",
  "license": "Apache-2.0",
  "dependencies": {
    "@mds-core/mds-logger": "^0.1.5",
    "@mds-core/mds-types": "^0.1.2",
    "@mds-core/mds-utils": "^0.1.5",
    "pg": "7.12.0"
  },
  "devDependencies": {
    "@mds-core/mds-test-data": "^0.1.5"
  }
}
