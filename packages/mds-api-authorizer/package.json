{
  "name": "@mds-core/mds-api-authorizer",
  "version": "0.1.5",
  "description": "MDS API Authorizer",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "test": "yarn test:eslint && yarn test:unit",
    "test:eslint": "eslint --fix --ignore-path ../../.gitignore '**/*.ts'",
    "test:unit": "nyc --check-coverage --exclude tests --extension .ts --lines 90 --reporter=text --reporter=html ts-mocha --project ../../tsconfig.json --require tsconfig-paths/register --require source-map-support/register --recursive tests/**/*.ts"
  },
  "keywords": [
    "mds"
  ],
  "author": "City of Los Angeles",
  "dependencies": {
    "@mds-core/mds-types": "^0.1.2",
    "jwt-decode": "2.2.0"
  },
  "devDependencies": {
    "@mds-core/mds-test-data": "^0.1.5"
  },
  "license": "Apache-2.0"
}
