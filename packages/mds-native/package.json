{
  "name": "@mds-core/mds-native",
  "description": "MDS Native Data Feed",
  "version": "0.0.3",
  "author": "City of Los Angeles",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "yarn watch server",
    "test": "yarn test:eslint && yarn test:unit",
    "test:eslint": "eslint --fix --ignore-path ../../.gitignore '**/*.ts'",
    "test:unit": "DOTENV_CONFIG_PATH=../../.env nyc --check-coverage --exclude tests --extension .ts --lines 85 --reporter=text --reporter=html ts-mocha --project ../../tsconfig.json --require tsconfig-paths/register --require source-map-support/register --require dotenv/config --recursive tests/**/*.ts",
    "watch": "nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec yarn watch:exec --",
    "watch:exec": "yarn build && DOTENV_CONFIG_PATH=../../.env ts-node -r dotenv/config"
  },
  "dependencies": {
    "@mds-core/mds-api-helpers": "^0.1.5",
    "@mds-core/mds-api-server": "^0.1.5",
    "@mds-core/mds-db": "^0.1.5",
    "@mds-core/mds-logger": "^0.1.5",
    "@mds-core/mds-providers": "^0.1.5",
    "@mds-core/mds-types": "^0.1.2",
    "@mds-core/mds-utils": "^0.1.5",
    "express": "4.17.1"
  },
  "devDependencies": {
    "@mds-core/mds-test-data": "^0.1.5"
  }
}
